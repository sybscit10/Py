class Shape:   
# Parent class
    pass


class Square(Shape):
    def area(self, side):
        return side * side


class Rectangle(Shape):
    def area(self, l, b):
        return l * b




# Using the classes
sq = Square()
print("Area of Square =", sq.area(5))


rect = Rectangle()
print("Area of Rectangle =", rect.area(6, 3))
