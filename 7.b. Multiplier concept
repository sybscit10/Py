class Numbers (object):
      def_init_(self, x, y):
        self.x=x
        self.y=y
        
def add (self):
    return self.x + self.y
    
def multiply(self):
    MULTIPLIER = 5
    return self.x* MULTIPLIER
    
num Numbers (3,5)
print("Additional:", num.add())
print("Multiplication with multiplier:", num.multiply())
